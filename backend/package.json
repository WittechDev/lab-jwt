{
    "name": "lab-jwt",
    "version": "1.0.0",
    "description": "API server using Node.js, Express, PostgreSQL, and JWT",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "migrate": "npx sequelize db:migrate",
        "migrate:down": "npx sequelize-cli db:migrate:undo",
        "seed": "npx sequelize db:seed:all"
    },
    "dependencies": {
        "@node-rs/argon2": "^2.0.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.11.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "license": "MIT"
}
